h2 = t "onboarding.get_started"

form submit="save"
  view "forms/selectInput" name="location" optionsTranslationRoot=false

  label = t "onboarding.dob"

  .account-dob-wrapper
  	view "forms/textInput" name="dobMonth" noLabelText=true
  	view "forms/textInput" name="dobDay" noLabelText=true
  	view "forms/textInput" name="dobYear" noLabelText=true

  view "forms/radioInput" name="sex"
  view "forms/textInput" name="gender"

  view "forms/saveButton" name="continue"
